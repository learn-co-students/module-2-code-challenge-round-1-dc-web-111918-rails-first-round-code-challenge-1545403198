<h1> Create a New Heroine! </h1>
<div>
<%= form_for @heroine do |f|%>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :super_name %>
    <%= f.text_field :super_name %>
    <br>
    <%= f.label :power %>
    <%= f.text_field :power %>
    <br>
    <%= f.submit 'Submit' %>
</div>

<% end %>
